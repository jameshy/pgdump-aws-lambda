{
  "name": "pgdump-aws-lambda",
  "version": "1.5.1",
  "description": "Lambda function for executing pg_dump and streaming the output to s3.",
  "main": "index.js",
  "dependencies": {
    "aws-sdk": "2.1658.0",
    "moment": "2.30.1"
  },
  "devDependencies": {
    "aws-sdk-mock": "6.0.4",
    "chai": " 4.4.1",
    "chai-as-promised": "7.1.2",
    "coveralls": "3.1.1",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "mocha": "10.6.0",
    "mock-spawn": "0.2.6",
    "mockdate": "3.0.5",
    "nyc": "17.0.0",
    "rewire": "7.0.0",
    "sinon": "18.0.0",
    "tmp": "0.2.3"
  },
