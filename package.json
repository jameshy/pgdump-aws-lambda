{
    "name": "pgdump-aws-lambda",
    "version": "1.5.1",
    "description": "Lambda function for executing pg_dump and streaming the output to s3.",
    "main": "index.js",
    "dependencies": {
        "moment": "2.29.4",
        "@aws-sdk/client-secrets-manager": "^3.599.0",
        "@aws-sdk/lib-storage": "^3.598.0",
        "@aws-sdk/client-s3": "^3.598.0",
        "@aws-sdk/rds-signer":"3.598.0" 
    },
    "devDependencies": {
        "aws-sdk-mock": "5.8.0",
        "chai": "4.3.6",
        "chai-as-promised": "7.1.1",
        "coveralls": "3.1.1",
        "eslint": "8.26.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-plugin-import": "2.26.0",
        "mocha": "10.1.0",
        "mock-spawn": "0.2.6",
        "mockdate": "3.0.5",
        "nyc": "15.1.0",
        "rewire": "6.0.0",
        "sinon": "14.0.1",
        "tmp": "0.2.1"
    },
    "scripts": {
